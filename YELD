		MenuOpen.Visible = false
		CloseMenu.Visible = true
	end)
	Back.MouseButton1Click:Connect(function()
		MenuOpen.Visible = true
		CloseMenu.Visible = false
	end)
	Unload.MouseButton1Click:Connect(function()
		ClosedUi()
		ZeroLib:Destroy()
	end)
	Minmizer.MouseButton1Click:Connect(function()
		MainMenu.Visible = false
		MenuOpen.Visible = true
	end)
	Hub.Size = UDim2.new(0.981, 0,0.128, 0)
	Close.MouseButton1Click:Connect(function()
		MainMenu.Visible = false
		MenuOpen.Visible = true
	end)
	--[[Loading]]--
	Loading.Visible = true
	task.delay(9,function()
		Loading.Visible = false
		MainMenu.Visible = true
	end)
	--[[Other Function]]--
	local Tab = {}
	local function ClearTab()
		for _,v in TabFrame:GetChildren() do
			if v:IsA('Frame') then
				v.Visible = false
			end
		end
	end
	TemplateTabButton.Visible = false
	function Tab.CreateTab(TabName)
		local TabClick = TemplateTabButton:Clone()
		TabClick.Parent = List
		TabClick.Visible = true
		TabClick.TextName.Text = TabName
		TabClick.Name = TabName
		local Tabbest = Template:Clone()
		Tabbest.Parent = TabFrame
		Tabbest.Name = TabName
		TabClick.MouseEnter:Connect(function()
			TabClick.BackgroundColor3 = ColorTheme[Theme.." Bright"]
		end)
		TabClick.MouseLeave:Connect(function()
			TabClick.BackgroundColor3 = ColorTheme[Theme]
		end)
		TabClick.HitBox.MouseButton1Click:Connect(function()
			ClearTab()
			Tabbest.Visible = true
		end)
		local UISInsert = {}
		-- Text , CallBack
		local TabScroll = Tabbest.ListFrame.ScrollList
		local Layout = TabScroll.UIListLayout

		TabScroll.CanvasSize = UDim2.new(0, 0, 0, Layout.AbsoluteContentSize.Y)
		TabScroll.ChildAdded:Connect(function()
			TabScroll.CanvasSize = UDim2.new(0, 0, 0, tonumber(Layout.AbsoluteContentSize.Y) + (tonumber(Layout.AbsoluteContentSize.Y) * 0.2))
		end)
		function UISInsert.TextLabel(...)
			local TableOf = {...}
			local TextSet = TableOf[1].Text
			local NewLabel = TextLabelTemplate:Clone()
			NewLabel.Parent = TabScroll
			NewLabel.LabelText.Text = TextSet
			NewLabel.Visible = true
		end
		-- Button Name , CallBack, MouseEffect
		function UISInsert.TextButton(...)
			local TableOf = {...}
			local Name = TableOf[1].Name
			local Fent = TableOf[1].Fonts or nil
			local NewButton = ButtonTemplate:Clone()
			NewButton.Parent = TabScroll
			NewButton.Name = Name
			NewButton.Visible = true
			NewButton.LabelText.Text = Name
			print()

			if Fent ~= nil then
				NewButton.LabelText.Font = Fent
			end
			NewButton.Button.MouseButton1Click:Connect(function()
				--[[Effect]]
				TableOf[1].CallBack()

				local Aspect = NewButton.UIAspectRatioConstraint
				local TW2 = TweenService:Create(Aspect,TweenInfo.new(0.05),{AspectRatio = 5.832})
				local TW1 =	TweenService:Create(Aspect,TweenInfo.new(0.05),{AspectRatio = 5.302})
				TW1:Play()
				TW2:Cancel()
				
				wait(0.3)
				TW1:Cancel()
				TW2:Play()

				--[[CallBack]]
			end)
		end
		-- Name , FirstBool , CallBack
		function UISInsert.Toggle(...)
			local TableOf = {...}
			local Name = TableOf[1].Name
			local LastBool = TableOf[1].FirstBool
			local NewToggle = ToggleTemplate:Clone()
			local Bool = Instance.new('BoolValue',NewToggle)
			Bool.Name = "Bool"
			NewToggle.Parent = TabScroll
			NewToggle.Name = Name
			NewToggle.Visible = true

			NewToggle.LabelText.Text = Name
			NewToggle.Bool.Value = LastBool
			local Value = NewToggle.Bool
			local ToggleButton:ImageButton = NewToggle.Toggle
			if Value.Value == true then
				ToggleButton.Image = 'rbxassetid://11896842923'
			else
				ToggleButton.Image = 'rbxassetid://9974174865'
			end
			ToggleButton.MouseButton1Click:Connect(function()
				Value.Value = not Value.Value
				if Value.Value == true then
					ToggleButton.Image = 'rbxassetid://11896842923'
				else
					ToggleButton.Image = 'rbxassetid://9974174865'
				end
				TableOf[1].CallBack(Value.Value)
			end)
		end
		--Name , Type , CallBack
		function UISInsert.TextBox(...)
			local TableOf = {...}
			local Name = TableOf[1].Name or "Name"
			local NewBox = TextBoxTemplate:Clone()
			local Type = TableOf[1].Type or "Normal"
			NewBox.Parent = TabScroll
			NewBox.Visible = true
			local Box:TextBox = NewBox.TextBox
			Box.ClearTextOnFocus  = false
			Box.TextColor3 = Color3.new(1, 1, 1)
			NewBox.LabelText.Text=Name
			Box.Font = Enum.Font.GothamBold

			Box.FocusLost:Connect(function(t)
				if t == true then
					if Type == 'Number' then
						if tonumber(Box.Text) then
							TableOf[1].CallBack(tonumber(Box.Text))
						else
							Box.Text = ''
						end
					else
						TableOf[1].CallBack(Box.Text)
					end
				else
					if Type == 'Number' then
						if tonumber(Box.Text) then
							TableOf[1].CallBack(tonumber(Box.Text))
						else
							Box.Text = ''
						end
					else
						TableOf[1].CallBack(Box.Text)
					end
				end
			end)
		end
		
		
		return UISInsert
	end
	
	local function Change(Colors)
		MainMenu.BackgroundColor3 = Colors
		TabFrame.BackgroundColor3 = Colors
		print('Changed')
		Hub.BackgroundColor3 = Colors
		
		for _,v in List:GetChildren() do
			if v:IsA('Frame') then
				v.BackgroundColor3 = Colors
			end
		end
		for _,v in TabFrame:GetDescendants() do
			if v:IsA('Frame') then
				v.BackgroundColor3 = Colors
			end
		end
		if Colors == Color3.new(1, 1, 1) then
			print('White Detect')
			for _,v in ZeroLib:GetDescendants() do
				if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
					v.TextColor3 = Color3.new(0, 0, 0)
				end
			end
		else
			for _,v in ZeroLib:GetDescendants() do
				if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
					v.TextColor3 = Color3.new(1, 1, 1)
				end
			end
		end
		wait(0.3)
		if Theme == 'Retro' then
			Colors = Color3.new(1, 1, 1)
			MainMenu.BackgroundColor3 = Color3.new(1, 1, 1)
			TabFrame.BackgroundColor3 = Colors
			Hub.BackgroundColor3 = Colors

			
		end
		if Theme == 'Cyber' then
			MainMenu.BackgroundColor3 = Color3.new(1, 1, 1)
			TabFrame.BackgroundColor3 = Colors
			Hub.BackgroundColor3 = Colors

			
		end
	end
	
	local function Retro()
		local UIGradient = Instance.new("UIGradient")
		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(170, 0, 255)), ColorSequenceKeypoint.new(0.09, Color3.fromRGB(170, 0, 127)), ColorSequenceKeypoint.new(0.31, Color3.fromRGB(170, 0, 255)), ColorSequenceKeypoint.new(0.63, Color3.fromRGB(170, 85, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(170, 16, 85))}
		UIGradient.Rotation = 45
		UIGradient.Name = "Retro"
		UIGradient.Parent = MainMenu
		local N1 = UIGradient:Clone()
		N1.Parent = TabFrame
		local N2 = UIGradient:Clone()
		N2.Parent = ThumbnailBackground
		local N3 = UIGradient:Clone()
		N3.Parent = Hub
		N3.Rotation = 90
	end
	local function Cyber()
		local UIGradient = Instance.new("UIGradient")
		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(44, 54, 112)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(12, 15, 50))}
		UIGradient.Parent = MainMenu
		local N1 = UIGradient:Clone()
		N1.Parent = TabFrame
		local N2 = UIGradient:Clone()
		N2.Parent = ThumbnailBackground
		local N3 = UIGradient:Clone()
		N3.Parent = Hub
		for _,v in List:GetChildren() do
			if v:IsA('Frame') then
				v.BackgroundColor3 = Color3.new(1, 0.180392, 0.85098)
			end
		end
	end
	
	function Tab.ChangeTheme(Themes:string)
		if MainMenu:FindFirstChildWhichIsA('UIGradient') then
			MainMenu:FindFirstChildWhichIsA('UIGradient'):Destroy()
		end
		if ThumbnailBackground:FindFirstChildWhichIsA('UIGradient') then
			ThumbnailBackground:FindFirstChildWhichIsA('UIGradient'):Destroy()
		end
		if TabFrame:FindFirstChildWhichIsA('UIGradient') then
			TabFrame:FindFirstChildWhichIsA('UIGradient'):Destroy()
		end
		if Hub:FindFirstChildWhichIsA('UIGradient') then
			Hub:FindFirstChildWhichIsA('UIGradient'):Destroy()
		end
		if Themes == "Dark" then
			Theme = Themes
			Change(ColorTheme[Themes])
		end
		if Themes == "Light" then
			Theme = Themes

			Change(ColorTheme[Themes])
		end
		if Themes == "Aqua" then
			Theme = Themes

			Change(ColorTheme[Themes])
		end
		if Themes == "Crystal" then
			Theme = Themes

			Change(ColorTheme[Themes])
		end
		if Themes == "Tropical" then
			Theme = Themes
			Change(ColorTheme[Themes])
		end
		if Themes == "Cyber" then
			Theme = Themes
			Cyber()
			Change(ColorTheme[Themes])
		end
		if Themes == "Frost" then
			Theme = Themes
			Change(ColorTheme[Themes])
		end
		if Themes == "Retro" then
			Retro()
			Theme = Themes
			Change(ColorTheme[Themes])
		end
	end
	
	return Tab
	
	
end

function Gui.Notification(NotifName,Texty)
	game.StarterGui:SetCore("SendNotification", {
		Title = NotifName;
		Text = Texty;
		Duration = "6";})
end



local Ui = Gui.New('Ugc Chekcer V1.7')
