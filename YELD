local Misc = Ui.CreateTab('Misc')
local Button = Misc.Toggle({Name = "Esp All Player",FirstBool = false,CallBack = function(Bool)
	if Bool == true then
		for _,v in Players:GetChildren() do
			if v:IsA('Player') then
				local Plr = v
				for _,Char in workspace:GetDescendants() do
					if Char.Name == Plr.Name and Char:FindFirstChild('Humanoid') then
						GiveEsp(Char.HumanoidRootPart)
					end
				end
			end
		end
	else
		ClearEsp()
	end
end})


local ToggleAgain = Misc.Toggle({
	Name = 'HighLight All Player',
	FirstBool = false,
	CallBack = function(Value)
		if Value == true then
			for _,v in Players:GetChildren() do
				if v:IsA('Player') then
					local Plr = v
					for _,Char in workspace:GetDescendants() do
						if Char.Name == Plr.Name and Char:FindFirstChild('Humanoid') then
							GiveHighligh(Char)
						end
					end
				end
			end
		else
			ClearHG()
		end
	end,
})
function ClearBeam()
	for _,v in workspace:GetDescendants() do
		if v.Name == "BeamEsp" and v:IsA('Beam') then
			v:Destroy()
		end
	end
end
local ToggleAgain = Misc.Toggle({
	Name = 'Beam All Player All Player',
	FirstBool = false,
	CallBack = function(Value)
		if Value == true then
			for _,v in Players:GetChildren() do
				if v:IsA('Player') then
					local Plr = v
					for _,Char in workspace:GetDescendants() do
						if Char.Name == Plr.Name and Char:FindFirstChild('Humanoid') then
							GiveBeam(Char.HumanoidRootPart)
						end
					end
				end
			end
		else
			ClearBeam()
		end
	end,
})

function ChangeFont(FT)
	for _,v in game:GetDescendants() do
		if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
			v.Font = FT
			if BoldText == true then
				v.FontFace.Bold = true
			end
		end
	end
end

local Fent = Ui.CreateTab('Font')
local TextLabel = Fent.TextLabel({
	Text = "The Font Will Changed To The Game",
})
local TextLabel = Fent.TextLabel({
	Text = "If you want reset, Go Rejoin",
})
function Bold(Bola)
	if Bola == true then
		for _,v in game:GetDescendants() do
			if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
				print('true')

				v.FontFace.Bold = true
			end
		end
	else
		
		for _,v in game:GetDescendants() do
			if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
				v.FontFace.Bold = false
			end
		end
	end
end
local ToggleAgain = Fent.Toggle({
	Name = 'Bold Font',
	FirstBool = false,
	CallBack = function(Value)
		Bold(Value)
		BoldText = Value
	end,
})
