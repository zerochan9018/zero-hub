	Name = "Toggle Walk Speed", 
	FirstBool = false, -- Changing mouse Icon When you enter the ui
	CallBack = function(Value)
		if Value == true then
			Players.LocalPlayer.Character.Humanoid.WalkSpeed = WlakSp
		else
			Players.LocalPlayer.Character.Humanoid.WalkSpeed = game.StarterPlayer.CharacterWalkSpeed
		end
	end,
})

