Tab3:AddDropdown({
	Name = "Teleport Totem",
	Default = "None",
	Options = {
		'None',
		'Aurora Totem',
		'SmokeScreen Totem',
		'Windset Totem',
		'Tempest Totem',
		'Sundial Totem',
		'Eclipse Totem',
		'Meteor Totem',
		'Billizard Totem',
		'Avalance Totem'
	},
	Callback = function(Value)
		_G.TeleportTotem = Value
		if Value ~='None' then
			Players.LocalPlayer.Character.HumanoidRootPart.Position = TeleportedTotem[Value]
		end
	end    
})


